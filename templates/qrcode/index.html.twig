{% extends 'base.html.twig' %}

{% block title %}Qrcode index{% endblock %}

{% block body %}
    <h1>Qrcode index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for qrcode in qrcodes %}
            <tr>
                <td>{{ qrcode.id }}</td>
                <td>{{ qrcode.nom }}</td>
                <td>
                    <a href="{{ path('app_qrcode_show', {'id': qrcode.id}) }}">show</a>
                    <a href="{{ path('app_qrcode_edit', {'id': qrcode.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_qrcode_new') }}">Create new</a>
{% endblock %}
